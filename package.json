{
    "name": "wdl",
    "displayName": "WDL Syntax Highlighting",
    "description": "Syntax Highlighting for the Workflow Definition Language",
    "version": "0.0.1",
    "publisher": "vdemario",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "wdl",
            "aliases": ["WDL", "wdl"],
            "extensions": [".wdl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "wdl",
            "scopeName": "source.wdl",
            "path": "./syntaxes/WDL.tmLanguage"
        }]
    }
}